{
  "name": "kytoonlabs/composer-cleanup",
  "version": "1.0.6",
  "description": "A Composer package to review Laravel applications and remove unused dependencies from vendor folder",
  "type": "composer-plugin",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Development Team",
      "email": "develop@kytoonlabs.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "composer-plugin-api": "^2.0",
    "nikic/php-parser": "^5.0",
    "symfony/finder": "^5.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "KytoonLabs\\ComposerCleanup\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KytoonLabs\\ComposerCleanup\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "KytoonLabs\\ComposerCleanup\\ComposerCleanupPlugin"
  },
  "scripts": {
    "test": "phpunit",
    "cleanup": "php bin/cleanup.php"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
